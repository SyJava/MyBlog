r\n          style=\"width: 400px;margin-right: 10px;\" @keydown.enter.native=\"search_name\"></el-input>\r\n          <el-button size=\"small\" type=\"primary\"  @click=\"search_name\" icon=\"el-icon-search\">搜索</el-button>\r\n          <el-button size=\"small\" type=\"success\"  @click=\"dialog_add = true\" icon=\"el-icon-plus\">新增</el-button>\r\n      </div>\r\n      <div>\r\n          <el-table\r\n          :data=\"linkData\"\r\n          >\r\n          <el-table-column\r\n            label=\"编号\"\r\n            width=\"200\">\r\n            <template slot-scope=\"scope\">\r\n              <span style=\"margin-left: 10px\">{{ scope.row.id }}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"友链图片\"\r\n            width=\"90\">\r\n            <template slot-scope=\"scope\">\r\n              <img :src=\"scope.row.linkPicture\" width=\"50\" height=\"50\" />\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"友链名称\"\r\n            width=\"200\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag size=\"medium\">{{ scope.row.linkName }}</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"友链地址\"\r\n            >\r\n            <template slot-scope=\"scope\">\r\n              <el-tag size=\"medium\">{{ scope.row.linkAddress }}</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <el-table-column label=\"操作\" fixed=\"right\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n\r\n      <!-- 分页 -->\r\n      <div style=\"margin-top: 20px;\" v-if=\"showPage\">\r\n        <el-pagination\r\n        background\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page.sync=\"currentPage\"\r\n        :page-size=\"pagesize\"\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n      </div>\r\n\r\n       <!-- 对话框 新建友链 -->\r\n       <el-dialog\r\n       title=\"新建友链\"\r\n       :visible.sync=\"dialog_add\"\r\n       width=\"30%\"\r\n       >\r\n       <el-form status-icon  label-width=\"100px\" :model=\"link_add\" ref=\"link_add\">\r\n        <el-form-item label=\"友链图片\" prop=\"linkPicture\"\r\n          :rules=\"{ required: true, message: '不能为空', trigger: 'blur' }\">\r\n          <el-input v-model=\"link_add.linkPicture\" placeholder=\"请输入友链图片\"></el-input>\r\n\r\n        </el-form-item>\r\n         <el-form-item label=\"友链名称\" prop=\"linkName\"\r\n          :rules=\"{ required: true, message: '不能为空', trigger: 'blur' }\">\r\n           <el-input v-model=\"link_add.linkName\" placeholder=\"请输入友链名称\"></el-input>\r\n         </el-form-item>\r\n         <el-form-item label=\"友链地址\" prop=\"linkAddress\"\r\n           :rules=\"{ required: true, message: '不能为空', trigger: 'blur' }\">\r\n            <el-input v-model=\"link_add.linkAddress\" placeholder=\"请输入友链名称\"></el-input>\r\n\r\n          </el-form-item>\r\n       </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n           <el-button @click=\"dialog_add = false\">取 消</el-button>\r\n           <el-button type=\"primary\" @click=\"addLink('link_add')\">确 定</el-button>\r\n         </span>\r\n     </el-dialog>\r\n\r\n      <!-- 对话框 编辑友链 -->\r\n      <el-dialog\r\n      title=\"编辑友链\"\r\n      :visible.sync=\"dialog_edit\"\r\n      width=\"30%\"\r\n      >\r\n      <el-form status-icon  label-width=\"100px\" :model=\"link_edit\" ref=\"link_edit\">\r\n        <el-form-item label=\"友链图片\" prop=\"linkPicture\"\r\n          :rules=\"{ required: true, message: '不能为空', trigger: 'blur' }\">\r\n          <el-input v-model=\"link_edit.linkPicture\" placeholder=\"请输入友链图片\"></el-input>\r\n\r\n        </el-form-item>\r\n         <el-form-item label=\"友链名称\" prop=\"linkName\"\r\n          :rules=\"{ required: true, message: '不能为空', trigger: 'blur' }\">\r\n           <el-input v-model=\"link_edit.linkName\" placeholder=\"请输入友链名称\"></el-input>\r\n         </el-form-item>\r\n         <el-form-item label=\"友链地址\" prop=\"linkAddress\"\r\n           :rules=\"{ required: true, message: '不能为空', trigger: 'blur' }\">\r\n            <el-input v-model=\"link_edit.linkAddress\" placeholder=\"请输入友链名称\"></el-input>\r\n\r\n          </el-form-item>\r\n       </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialog_edit = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editLink('link_edit')\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LinksBlog',\r\n  data () {\r\n   return {\r\n      linkData:[],  //友链数据\r\n      currentPage: 1,  //当前页\r\n      total:0, //总记录数\r\n      pagesize:5, //页面大小\r\n      input_name:'', //搜索框值\r\n      dialog_add: false, //添加友链的对话框\r\n      dialog_edit: false, //编辑友链的对话框\r\n      link_add:{\r\n        linkName:'',  //友链名称\r\n        linkPicture:'',\r\n        linkAddress:'',\r\n      },\r\n      link_edit:{\r\n        linkName:'',  //友链名称\r\n        linkPicture:'',\r\n        linkAddress:'',\r\n      },\r\n      showPage: true, //是否显示分页\r\n   }\r\n  },\r\n  mounted() {\r\n    this.initLink();\r\n  },\r\n  methods:{\r\n    //初始化友链数据\r\n    initLink(){\r\n      const _this = this\r\n      this.$axios.get('/links/getLinksByPage?current=' + this.currentPage + '&size=' + this.pagesize).then(resp=>{\r\n        console.log(resp)\r\n        _this.linkData = resp.data.data.records\r\n        _this.total = resp.data.data.total\r\n      })\r\n    },\r\n    //添加友链\r\n    addLink(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          const _this = this\r\n          this.$axios.post('/links/saveLink',this.link_add).then(resp=>{\r\n            console.log(resp)\r\n            _this.initLink()\r\n            _this.link_add = ''\r\n            _this.dialog_add = false\r\n            _this.$message.success(resp.data.msg)\r\n          })\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //编辑友链\r\n    handleEdit(index, row) {\r\n      console.log(index, row);\r\n      this.link_edit = row\r\n      this.dialog_edit = true\r\n    },\r\n    //更新友链\r\n    editLink(formName2) {\r\n      this.$refs[formName2].validate((valid) => {\r\n        if (valid) {\r\n          const _this = this\r\n          this.$axios.put('/links/updateLink',this.link_edit).then(resp=>{\r\n            console.log(resp)\r\n            _this.initLink()\r\n            _this.dialog_edit = false\r\n            _this.$message.success(resp.data.msg)\r\n          })\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //删除友链\r\n    handleDelete(index, row) {\r\n      console.log(index, row);\r\n      const _this = this\r\n      this.$confirm('此操作将永久删除《' + row.linkName  + '》, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.delete('/links/deleteLinkById?id=' + row.id).then(resp=>{\r\n            if(resp){\r\n              this.initLink()\r\n            }\r\n            _this.$message.success(resp.data.msg)\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n\r\n    },\r\n    //搜索友链\r\n    search_name(){\r\n       const _this = this\r\n       this.getRequest('/links/getLinksByName?name=' + this.input_name).then(resp=>{\r\n         console.log(resp)\r\n         if(_this.input_name == ''){\r\n           _this.initLink();\r\n           _this.showPage = true\r\n         }else{\r\n          _this.linkData = resp.obj\r\n          _this.showPage = false\r\n          _this.input_name = ''\r\n         }\r\n\r\n\r\n       })\r\n    },\r\n    //分页\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.currentPage = val\r\n      this.initLink()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}