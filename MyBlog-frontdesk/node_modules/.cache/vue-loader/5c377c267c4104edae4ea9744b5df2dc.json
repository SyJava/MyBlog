   <el-button size=\"small\" type=\"primary\"  @click=\"search_name\" icon=\"el-icon-search\">搜索</el-button>\r\n          <el-button size=\"small\" type=\"success\"  @click=\"dialog_add = true\" icon=\"el-icon-plus\">新增</el-button>\r\n      </div>\r\n      <div>\r\n          <el-table\r\n          :data=\"typeData\"\r\n          style=\"width: 70%\">\r\n          <el-table-column\r\n            label=\"编号\"\r\n            width=\"230\">\r\n            <template slot-scope=\"scope\">\r\n              <span style=\"margin-left: 10px\">{{ scope.row.id }}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"名称\"\r\n            width=\"230\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag size=\"medium\">{{ scope.row.name }}</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n\r\n      <!-- 分页 -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"pagesizes\"\r\n          :page-size=\"pagesize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n       <!-- 对话框 新建分类专栏 -->\r\n       <el-dialog\r\n       title=\"新建分类专栏\"\r\n       :visible.sync=\"dialog_add\"\r\n       width=\"30%\"\r\n       >\r\n       <el-form status-icon  label-width=\"120px\">\r\n         <el-form-item label=\"分类专栏名\">\r\n           <el-input v-model=\"add_type.name\" placeholder=\"请输入要新建的分类专栏名\"\r\n           :rules=\"{\r\n            required: true, message: '文章类型不能为空', trigger: 'blur'\r\n            }\"></el-input>\r\n         </el-form-item>\r\n       </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n           <el-button @click=\"dialog_add = false\">取 消</el-button>\r\n           <el-button type=\"primary\" @click=\"addNewType\">确 定</el-button>\r\n         </span>\r\n     </el-dialog>\r\n\r\n      <!-- 对话框 编辑分类专栏 -->\r\n      <el-dialog\r\n      title=\"编辑分类专栏\"\r\n      :visible.sync=\"dialog_edit\"\r\n      width=\"30%\"\r\n      >\r\n      <el-form status-icon  label-width=\"120px\">\r\n        <el-form-item label=\"分类专栏名\">\r\n          <el-input v-model=\"edit_type.name\"\r\n          :rules=\"{\r\n            required: true, message: '文章类型不能为空', trigger: 'blur'\r\n            }\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialog_edit = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"updateType\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CategoryBlog',\r\n  inject:['reload'],\r\n  data () {\r\n   return {\r\n      typeData:[],  //分类数据\r\n     currentPage: 1,  //当前页\r\n     total:0, //总记录数\r\n     pagesize:5, //页面大小\r\n     pagesizes:[5,10,15], //页面数组\r\n      input_name:'', //搜索框值\r\n      dialog_add: false, //添加分类的对话框\r\n      dialog_edit: false, //编辑分类的对话框\r\n      add_type:{\r\n        name:''//分类名称\r\n      },\r\n     edit_type:{\r\n       id:'',\r\n       name:''//分类名称\r\n     },\r\n      showPage: true, //是否显示分页\r\n   }\r\n  },\r\n  mounted() {\r\n    this.initType();\r\n  },\r\n  methods:{\r\n    //初始化分类数据\r\n    initType(){\r\n      const _this = this\r\n      this.$axios.get('/type/getTypeByPage?current=' + this.currentPage + '&size=' + this.pagesize).then(resp=>{\r\n        console.log(resp.data)\r\n        _this.typeData = resp.data.data.records\r\n        _this.total = resp.data.data.total\r\n      })\r\n    },\r\n    //编辑分类\r\n    handleEdit(index, row) {\r\n      const _this = this\r\n      console.log(index, row);\r\n      this.dialog_edit = true\r\n      _this.edit_type.id=row.id\r\n      _this.edit_type.name=row.name\r\n    },\r\n    //更新分类\r\n    updateType(){\r\n      const _this = this\r\n      this.$axios.put('/type/updateType',this.edit_type).then(resp=>{\r\n        if(resp){\r\n          this.initType()\r\n          _this.dialog_edit = false\r\n        }\r\n        this.reload() // 刷新当前页面\r\n      })\r\n    },\r\n    //删除分类\r\n    handleDelete(index, row) {\r\n      console.log(\"ewe\" + row);\r\n      const _this = this\r\n      this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.delete('/type/deleteTypeById?id=' + row.id).then(resp=>{\r\n            if(resp){\r\n              this.initType()\r\n            }\r\n            this.reload() // 刷新当前页面\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n\r\n    },\r\n    //添加分类\r\n    addNewType(){\r\n      const _this = this\r\n      this.$axios.post('/type/addType',this.add_type.name).then(resp=>{\r\n        console.log(resp)\r\n        _this.add_type.name =''\r\n        _this.dialog_add = false\r\n        this.reload() // 刷新当前页面\r\n      })\r\n    },\r\n    //搜索分类\r\n    search_name(){\r\n       const _this = this\r\n       this.getRequest('/type/getTypeByName?name=' + this.input_name).then(resp=>{\r\n         console.log(resp)\r\n         if(_this.input_name == ''){\r\n           _this.initType();\r\n           _this.showPage = true\r\n         }else{\r\n          _this.typeData = resp.obj\r\n          _this.showPage = false\r\n          _this.input_name = ''\r\n         }\r\n\r\n\r\n       })\r\n    },    // 分页的当前页\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.initType()\r\n    },\r\n    //每页多少条\r\n    handleSizeChange(val) {\r\n      this.pagesize = val\r\n      this.initType()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}