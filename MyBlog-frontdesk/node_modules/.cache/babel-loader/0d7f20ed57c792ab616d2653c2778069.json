ACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAJA;AAKA,KATA;AAUA;AACA,IAAA,UAXA,sBAWA,KAXA,EAWA,GAXA,EAWA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,QAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,WAAA,QAAA,CAAA,OAAA,GAAA,GAAA,CAAA,OAAA;AACA,KAhBA;AAiBA;AACA,IAAA,SAlBA,uBAkBA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA;;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA;;AACA,QAAA,KAAA,CAAA,MAAA;AACA,OANA;AAOA,KA3BA;AA4BA;AACA,IAAA,YA7BA,wBA6BA,KA7BA,EA6BA,GA7BA,EA6BA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,2BAAA,GAAA,CAAA,KAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,OAAA;AACA;;AACA,UAAA,MAAA,CAAA,MAAA,GAJA,CAIA;;AACA,SALA;AAMA,OAXA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAhBA;AAkBA,KAlDA;AAmDA;AACA,IAAA,SApDA,uBAoDA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,aAAA,EAAA,KAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,MAAA,GAJA,CAIA;;AACA,OALA;AAMA,KA5DA;AA6DA;AACA,IAAA,WA9DA,yBA8DA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,4BAAA,KAAA,UAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,KAAA,CAAA,UAAA,IAAA,EAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA;;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,IAAA,CAAA,GAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,EAAA;AACA;AAGA,OAZA;AAaA,KA7EA;AA8EA;AACA,IAAA,mBA/EA,+BA+EA,GA/EA,EA+EA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,OAAA;AACA;AAnFA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n      <div style=\"margin-top: 20px;\">\r\n          <!-- 搜索 -->\r\n          <el-input size=\"small\" v-model=\"input_name\" placeholder=\"请输入标签名称，可回车搜索...\" prefix-icon=\"el-icon-search\"\r\n          style=\"width: 400px;margin-right: 10px;\" @keydown.enter.native=\"search_name\"></el-input>\r\n          <el-button size=\"small\" type=\"primary\"  @click=\"search_name\" icon=\"el-icon-search\">搜索</el-button>\r\n          <el-button size=\"small\" type=\"success\"  @click=\"dialog_add = true\" icon=\"el-icon-plus\">新增</el-button>\r\n      </div>\r\n      <div>\r\n          <el-table\r\n          :data=\"tagData\"\r\n          style=\"width: 70%\">\r\n          <el-table-column\r\n            label=\"编号\"\r\n            width=\"230\">\r\n            <template slot-scope=\"scope\">\r\n              <span style=\"margin-left: 10px\">{{ scope.row.tagId }}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"名称\"\r\n            width=\"230\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag size=\"medium\">{{ scope.row.tagName }}</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n\r\n      <!-- 分页 -->\r\n      <div style=\"margin-top: 20px;\" v-if=\"showPage\">\r\n        <el-pagination\r\n        background\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page.sync=\"currentPage\"\r\n        :page-size=\"pagesize\"\r\n        layout=\"total, prev, pager, next\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n      </div>\r\n\r\n       <!-- 对话框 新建标签 -->\r\n       <el-dialog\r\n       title=\"新建标签\"\r\n       :visible.sync=\"dialog_add\"\r\n       width=\"30%\"\r\n       >\r\n       <el-form status-icon  label-width=\"120px\">\r\n         <el-form-item label=\"标签名称\">\r\n           <el-input v-model=\"tag_add.tagName\" placeholder=\"请输入要新建的标签名称\"\r\n           :rules=\"{\r\n            required: true, message: '不能为空', trigger: 'blur'\r\n            }\"></el-input>\r\n         </el-form-item>\r\n       </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n           <el-button @click=\"dialog_add = false\">取 消</el-button>\r\n           <el-button type=\"primary\" @click=\"addNewTag\">确 定</el-button>\r\n         </span>\r\n     </el-dialog>\r\n\r\n      <!-- 对话框 编辑标签 -->\r\n      <el-dialog\r\n      title=\"编辑标签\"\r\n      :visible.sync=\"dialog_edit\"\r\n      width=\"30%\"\r\n      >\r\n      <el-form status-icon  label-width=\"120px\">\r\n        <el-form-item label=\"标签名称\">\r\n          <el-input v-model=\"tag_edit.tagName\"\r\n          :rules=\"{\r\n            required: true, message: '不能为空', trigger: 'blur'\r\n            }\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialog_edit = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"updateTag\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'TagBlog',\r\n  inject:['reload'],\r\n  data () {\r\n   return {\r\n      tagData:[],  //标签数据\r\n      currentPage: 1,  //当前页\r\n      total:0, //总记录数\r\n      pagesize:5, //页面大小\r\n      input_name:'', //搜索框值\r\n      dialog_add: false, //添加标签的对话框\r\n      dialog_edit: false, //编辑标签的对话框\r\n      tag_add:{\r\n        tagName:'',  //标签名称\r\n      },\r\n      tag_edit:{\r\n        tagId:'',\r\n        tagName:''\r\n      },\r\n      showPage: true, //是否显示分页\r\n   }\r\n  },\r\n  mounted() {\r\n    this.initTag();\r\n  },\r\n  methods:{\r\n    //初始化分类数据\r\n    initTag(){\r\n      const _this = this\r\n      this.$axios.get('/tag/getTagByPage?current=' + this.currentPage + '&size=' + this.pagesize).then(resp=>{\r\n        console.log(resp)\r\n        _this.tagData = resp.data.data.records\r\n        _this.total = resp.data.data.total\r\n      })\r\n    },\r\n    //编辑分类\r\n    handleEdit(index, row) {\r\n      console.log(index, row);\r\n      this.dialog_edit = true\r\n      this.tag_edit.id = row.id\r\n      this.tag_edit.tagName= row.tagName\r\n    },\r\n    //更新分类\r\n    updateTag(){\r\n      const _this = this\r\n      this.$axios.put('/tag/updateTag',this.tag_edit).then(resp=>{\r\n        if(resp){\r\n          this.initTag()\r\n          _this.dialog_edit = false\r\n        }\r\n        _this.reload()\r\n      })\r\n    },\r\n    //删除分类\r\n    handleDelete(index, row) {\r\n      console.log(index, row);\r\n      const _this = this\r\n      this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.delete('/tag/deleteTagById?id=' + row.tagId).then(resp=>{\r\n            if(resp){\r\n              this.initTag()\r\n            }\r\n            this.reload() // 刷新当前页面\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n\r\n    },\r\n    //添加标签\r\n    addNewTag(){\r\n      const _this = this\r\n      this.$axios.post('/tag/addTag',this.tag_add.tagName).then(resp=>{\r\n        console.log(resp)\r\n        _this.input_name =''\r\n        _this.dialog_add = false\r\n        this.reload() // 刷新当前页面\r\n      })\r\n    },\r\n    //搜索分类\r\n    search_name(){\r\n       const _this = this\r\n       this.axios.get('/tag/getTagByName?name=' + this.input_name).then(resp=>{\r\n         console.log(resp)\r\n         if(_this.input_name == ''){\r\n           _this.initTag();\r\n           _this.showPage = true\r\n         }else{\r\n          _this.tagData = resp.obj\r\n          _this.showPage = false\r\n          _this.input_name = ''\r\n         }\r\n\r\n\r\n       })\r\n    },\r\n    //分页\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.currentPage = val\r\n      this.initTag()\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/blogs"}]}