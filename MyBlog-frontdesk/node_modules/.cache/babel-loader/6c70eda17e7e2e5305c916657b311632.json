QAAA,CAHA,CAIA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,cAAA;AACA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,+CAAA;AACA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,8CAAA;AACA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,iCAAA;AACA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,gDAAA;AACA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,+CAAA;AACA;;AACA,UAAA,KAAA,OAAA,IAAA,GAAA,EAAA;AAAA;AACA,QAAA,OAAA,IAAA,cAAA;AACA,OAzBA,CA0BA;AACA;;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,OAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,EAFA,CAGA;;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA,CALA,CAKA;;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,SAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAPA,CAOA;AACA;AACA,OAXA;AAYA,KAzGA;AA0GA;AACA,IAAA,mBA3GA,+BA2GA,GA3GA,EA2GA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,SAAA;AACA,KA/GA;AAgHA;AACA,IAAA,gBAjHA,4BAiHA,GAjHA,EAiHA;AACA,MAAA,OAAA,CAAA,GAAA,wBAAA,GAAA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,SAAA;AACA;AArHA;AArBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n        <!-- 博客文章 -->\r\n        <el-table\r\n            v-loading=\"loading\"\r\n            element-loading-text=\"正在加载...\"\r\n            element-loading-spinner=\"el-icon-loading\"\r\n            element-loading-background=\"rgba(0, 0, 0, 0.8)\"\r\n            :data=\"blogsData\"\r\n            stripe\r\n            style=\"width: 100%\">\r\n            <el-table-column label=\"文章列表\">\r\n            <template slot-scope=\"scope\">\r\n                <el-card  class=\"box-card\">\r\n                <div style=\"font-size: 18px;\">\r\n                    <!-- v-if=\"!scope.row.shareStatement\" 这里根据shareStatement属性判断文章是否为草稿 -->\r\n                    <el-button size=\"mini\" v-if=\"!scope.row.shareStatement\" type=\"info\" icon=\"el-icon-edit\" circle></el-button>\r\n                    {{scope.row.title}}\r\n                </div>\r\n                <div style=\"margin-top: 10px;\">\r\n\r\n                    <el-tag size=\"small\" style=\"margin-right: 10px;\"  v-if=\"scope.row.isDelete\" type=\"danger\">已删除</el-tag>\r\n                    <el-tag size=\"small\" style=\"margin-right: 10px;\"  v-if=\"scope.row.shareStatement\" type=\"warning\">草稿</el-tag>\r\n                    <el-tag style=\"margin-right: 10px;\" v-if=\"!scope.row.shareStatement\" size=\"small\">{{scope.row.isOriginal ? '原创':'转载'}}</el-tag>\r\n                    <el-tag style=\"margin-right: 20px;\" v-if=\"!scope.row.shareStatement\" size=\"small\" type=\"info\">{{scope.row.isPublished  ? '公开' : '私密'}}</el-tag>\r\n<!--                     <el-tag style=\"margin-right: 20px;\" v-if=\"scope.row.shareStatement\" size=\"small\" type=\"success\">{{scope.row.typeId}}</el-tag>-->\r\n                    <i style=\"margin-right: 20px;\" class=\"el-icon-view\"> {{scope.row.views}} </i>\r\n                    <i style=\"margin-right: 20px;\" class=\"el-icon-chat-square\"> {{scope.row.commentCount}} </i>\r\n                    <i style=\"margin-right: 20px;\" class=\"el-icon-date\"> {{scope.row.createTime}}</i>\r\n\r\n                    <el-button v-if=\"!scope.row.isDelete\" style=\"float: right;\" type=\"danger\" size=\"mini\" @click=\"logicDeleteBlog(scope.row.blogId)\">删除</el-button>\r\n                    <router-link :to=\" '/admin/editblog/' + scope.row.blogId \">\r\n                      <el-button v-if=\"!scope.row.isDelete\" style=\"float: right; margin-right: 10px;\" type=\"primary\" size=\"mini\">编辑</el-button>\r\n                    </router-link>\r\n                    <el-button v-if=\"scope.row.isDelete\" style=\"float: right;\" type=\"danger\" size=\"mini\" @click=\"deleteBlog(scope.row.blogId)\">删除</el-button>\r\n                    <el-button v-if=\"scope.row.isDelete\" style=\"float: right; margin-right: 10px;\" @click=\"recoveryBlog(scope.row.blogId)\" type=\"primary\" size=\"mini\">还原</el-button>\r\n\r\n\r\n                </div>\r\n\r\n                </el-card>\r\n            </template>\r\n\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n       <!-- 分页 -->\r\n       <div style=\"margin-top: 20px;\">\r\n              <el-pagination\r\n                background\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"currentPage\"\r\n                :page-sizes=\"pagesizes\"\r\n                :page-size=\"pagesize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"total\">\r\n              </el-pagination>\r\n       </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'BlogListCom',\r\n  inject:['reload'],\r\n  data () {\r\n   return {\r\n    blogsData: [], //文章数据\r\n    currentPage: 1,  //当前页\r\n    total:0, //总记录数\r\n    pagesize:10, //页面大小\r\n    pagesizes:[10,20,30], //页面数组\r\n    t2index:0, //选项卡index\r\n    loading:false, //加载样式\r\n   }\r\n  },\r\n  props:[\"tindex\"],\r\n  mounted() {\r\n    this.initIndex();\r\n    this.initBlogs(); // 调用初始化博客数据\r\n\r\n  },\r\n\r\n  methods:{\r\n    //删除博客(逻辑删除)\r\n    logicDeleteBlog(id){\r\n      const _this = this\r\n      this.$confirm('此操作将删除该文章, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.delete('/blog/logicDeleteBlog?id=' + id).then(resp=>{\r\n            _this.$message.success(\"删除成功\")\r\n            this.reload() // 刷新当前页面\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n    },\r\n    //彻底删除博客\r\n    deleteBlog(id){\r\n      const _this = this\r\n\r\n      this.$confirm('此操作将彻底删除该文章, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.delete('/blog/deleteBlog?id=' + id).then(resp=>{\r\n            _this.$message.success(resp.data.msg)\r\n            this.reload() // 刷新当前页面\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n    },\r\n    //还原博客\r\n    recoveryBlog(id){\r\n      const _this = this\r\n      this.$confirm('确定要还原该博客吗?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.post('/blog/recoveryBlog?id=' + id).then(resp=>{\r\n            _this.$message.success(resp.data.msg)\r\n            this.reload() // 刷新当前页面\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消还原'\r\n          });\r\n        });\r\n    },\r\n    //组件的index\r\n    initIndex(){\r\n        this.t2index = this.tindex  //保存父组件传过来的值\r\n    },\r\n    // 初始化【全部】博客的数据\r\n    initBlogs(){\r\n      const _this = this\r\n      // 通用路由\r\n      var baseurl = '/blog/getByPage?current=' + this.currentPage + '&size=' + this.pagesize\r\n      //通过条件拼接路由\r\n      if(this.t2index == \"0\"){ //全部\r\n        baseurl +=' &isDelete=0'\r\n      }\r\n      if(this.t2index == \"1\"){ //原创\r\n        baseurl +=' &isOriginal=1 &shareStatement=0  &isDelete=0'\r\n      }\r\n      if(this.t2index == \"2\"){ //转载\r\n        baseurl +=' &isOriginal=0 &shareStatement=0 &isDelete=0'\r\n      }\r\n      if(this.t2index == \"3\"){  //草稿\r\n        baseurl +=' &shareStatement=1  &isDelete=0'\r\n      }\r\n      if(this.t2index == \"4\"){ //公开\r\n        baseurl +=' &isPublished=1 &shareStatement=0  &isDelete=0'\r\n      }\r\n      if(this.t2index == \"5\"){ //私密\r\n        baseurl +=' &isPublished=0 &shareStatement=0 &isDelete=0'\r\n      }\r\n      if(this.t2index == \"6\"){ //回收站\r\n        baseurl +=' &isDelete=1'\r\n      }\r\n      // console.log(baseurl)\r\n      //显示加载样式\r\n      this.loading = true\r\n      this.$axios.get(baseurl).then(resp=>{\r\n        // console.log(baseurl)\r\n        if(resp){\r\n          console.log(resp)\r\n          console.log(resp.data.data)\r\n          //隐藏加载样式\r\n          _this.loading = false\r\n          _this.blogsData = resp.data.data.records  //将获取到的后端的值赋值给blogsData\r\n          console.log(_this.blogsData)\r\n          _this.total = resp.data.data.total   // 保存一下总记录数，用于前端展示\r\n        }\r\n      })\r\n    },\r\n    // 分页的当前页\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.currentPage = val\r\n      this.initBlogs()\r\n    },\r\n    //每页多少条\r\n    handleSizeChange(val) {\r\n        console.log(`每页 ${val} 条`);\r\n        this.pagesize = val\r\n        this.initBlogs()\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/blogs"}]}