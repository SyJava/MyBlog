GAAA,QAAA,GAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAJA;AAKA,KATA;AAUA;AACA,IAAA,UAXA,sBAWA,KAXA,EAWA,GAXA,EAWA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,SAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,MAAA,KAAA,CAAA,SAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,KAjBA;AAkBA;AACA,IAAA,UAnBA,wBAmBA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,kBAAA,EAAA,KAAA,SAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA;;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA;;AACA,QAAA,MAAA,CAAA,MAAA,GALA,CAKA;;AACA,OANA;AAOA,KA5BA;AA6BA;AACA,IAAA,YA9BA,wBA8BA,KA9BA,EA8BA,GA9BA,EA8BA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,6BAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA;AACA;;AACA,UAAA,MAAA,CAAA,MAAA,GAJA,CAIA;;AACA,SALA;AAMA,OAXA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAhBA;AAkBA,KAlDA;AAmDA;AACA,IAAA,UApDA,wBAoDA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,eAAA,EAAA,KAAA,QAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,IAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,MAAA,GAJA,CAIA;;AACA,OALA;AAMA,KA5DA;AA6DA;AACA,IAAA,WA9DA,yBA8DA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,UAAA,CAAA,8BAAA,KAAA,UAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,KAAA,CAAA,UAAA,IAAA,EAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA;;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,GAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,EAAA;AACA;AAGA,OAZA;AAaA,KA7EA;AA6EA;AACA,IAAA,mBA9EA,+BA8EA,GA9EA,EA8EA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,QAAA;AACA,KAjFA;AAkFA;AACA,IAAA,gBAnFA,4BAmFA,GAnFA,EAmFA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,QAAA;AACA;AAtFA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n      <div style=\"margin-top: 20px;\">\r\n          <!-- 搜索 -->\r\n          <el-input size=\"small\" v-model=\"input_name\" placeholder=\"请输入分类名称，可回车搜索...\" prefix-icon=\"el-icon-search\"\r\n          style=\"width: 400px;margin-right: 10px;\" @keydown.enter.native=\"search_name\"></el-input>\r\n          <el-button size=\"small\" type=\"primary\"  @click=\"search_name\" icon=\"el-icon-search\">搜索</el-button>\r\n          <el-button size=\"small\" type=\"success\"  @click=\"dialog_add = true\" icon=\"el-icon-plus\">新增</el-button>\r\n      </div>\r\n      <div>\r\n          <el-table\r\n          :data=\"typeData\"\r\n          style=\"width: 70%\">\r\n          <el-table-column\r\n            label=\"编号\"\r\n            width=\"230\">\r\n            <template slot-scope=\"scope\">\r\n              <span style=\"margin-left: 10px\">{{ scope.row.id }}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"名称\"\r\n            width=\"230\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag size=\"medium\">{{ scope.row.name }}</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n\r\n      <!-- 分页 -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"pagesizes\"\r\n          :page-size=\"pagesize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n       <!-- 对话框 新建分类专栏 -->\r\n       <el-dialog\r\n       title=\"新建分类专栏\"\r\n       :visible.sync=\"dialog_add\"\r\n       width=\"30%\"\r\n       >\r\n       <el-form status-icon  label-width=\"120px\">\r\n         <el-form-item label=\"分类专栏名\">\r\n           <el-input v-model=\"add_type.name\" placeholder=\"请输入要新建的分类专栏名\"\r\n           :rules=\"{\r\n            required: true, message: '文章类型不能为空', trigger: 'blur'\r\n            }\"></el-input>\r\n         </el-form-item>\r\n       </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n           <el-button @click=\"dialog_add = false\">取 消</el-button>\r\n           <el-button type=\"primary\" @click=\"addNewType\">确 定</el-button>\r\n         </span>\r\n     </el-dialog>\r\n\r\n      <!-- 对话框 编辑分类专栏 -->\r\n      <el-dialog\r\n      title=\"编辑分类专栏\"\r\n      :visible.sync=\"dialog_edit\"\r\n      width=\"30%\"\r\n      >\r\n      <el-form status-icon  label-width=\"120px\">\r\n        <el-form-item label=\"分类专栏名\">\r\n          <el-input v-model=\"edit_type.name\"\r\n          :rules=\"{\r\n            required: true, message: '文章类型不能为空', trigger: 'blur'\r\n            }\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialog_edit = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"updateType\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CategoryBlog',\r\n  inject:['reload'],\r\n  data () {\r\n   return {\r\n      typeData:[],  //分类数据\r\n     currentPage: 1,  //当前页\r\n     total:0, //总记录数\r\n     pagesize:5, //页面大小\r\n     pagesizes:[5,10,15], //页面数组\r\n      input_name:'', //搜索框值\r\n      dialog_add: false, //添加分类的对话框\r\n      dialog_edit: false, //编辑分类的对话框\r\n      add_type:{\r\n        name:''//分类名称\r\n      },\r\n     edit_type:{\r\n       id:'',\r\n       name:''//分类名称\r\n     },\r\n      showPage: true, //是否显示分页\r\n   }\r\n  },\r\n  mounted() {\r\n    this.initType();\r\n  },\r\n  methods:{\r\n    //初始化分类数据\r\n    initType(){\r\n      const _this = this\r\n      this.$axios.get('/type/getTypeByPage?current=' + this.currentPage + '&size=' + this.pagesize).then(resp=>{\r\n        console.log(resp.data)\r\n        _this.typeData = resp.data.data.records\r\n        _this.total = resp.data.data.total\r\n      })\r\n    },\r\n    //编辑分类\r\n    handleEdit(index, row) {\r\n      const _this = this\r\n      console.log(index, row);\r\n      this.dialog_edit = true\r\n      _this.edit_type.id=row.id\r\n      _this.edit_type.name=row.name\r\n    },\r\n    //更新分类\r\n    updateType(){\r\n      const _this = this\r\n      this.$axios.put('/type/updateType',this.edit_type).then(resp=>{\r\n        if(resp){\r\n          this.initType()\r\n          _this.dialog_edit = false\r\n        }\r\n        this.reload() // 刷新当前页面\r\n      })\r\n    },\r\n    //删除分类\r\n    handleDelete(index, row) {\r\n      const _this = this\r\n      this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$axios.delete('/type/deleteTypeById?id=' + row.id).then(resp=>{\r\n            if(resp){\r\n              this.initType()\r\n            }\r\n            this.reload() // 刷新当前页面\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n\r\n    },\r\n    //添加分类\r\n    addNewType(){\r\n      const _this = this\r\n      this.$axios.post('/type/addType',this.add_type.name).then(resp=>{\r\n        console.log(resp)\r\n        _this.add_type.name =''\r\n        _this.dialog_add = false\r\n        this.reload() // 刷新当前页面\r\n      })\r\n    },\r\n    //搜索分类\r\n    search_name(){\r\n       const _this = this\r\n       this.getRequest('/type/getTypeByName?name=' + this.input_name).then(resp=>{\r\n         console.log(resp)\r\n         if(_this.input_name == ''){\r\n           _this.initType();\r\n           _this.showPage = true\r\n         }else{\r\n          _this.typeData = resp.obj\r\n          _this.showPage = false\r\n          _this.input_name = ''\r\n         }\r\n\r\n\r\n       })\r\n    },    // 分页的当前页\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.initType()\r\n    },\r\n    //每页多少条\r\n    handleSizeChange(val) {\r\n      this.pagesize = val\r\n      this.initType()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/blogs"}]}