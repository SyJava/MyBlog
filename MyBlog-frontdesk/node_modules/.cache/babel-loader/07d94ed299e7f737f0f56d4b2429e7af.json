\n\t\t\t\tthis.$refs[formName].resetFields();\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n.el-form {\r\n\twidth: 420px;\r\n\tmargin: 50px auto;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}